import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
      int num,squar,digit=0;
      Scanner scn=new Scanner(System.in);
      num=scn.nextInt();
        while(num>1){
                digit=num%10;
                 squar=digit*digit;
                num/=10;
                System.out.println("The squar :"+squar);
            }
            
        }
}       
       
