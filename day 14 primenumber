import java.util.*;
class HelloWorld {
 public static void main(String[] args) {
 int number=0,num;
 Scanner scn=new Scanner(System.in);
 System.out.println("Enter the num:");
 num=scn.nextInt();
 for(int i=2;i<=num/2;i++){
 if(num%i==0){
System.out.println("Number is not prime ");
number=1;
break;
 }
 }
if(number==0){
 System.out.println("Number is prime ");   
 }
 
    }
}
