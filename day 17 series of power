import java .util.*;
class HelloWorld {
    public static void main(String[] args) {
        int num,i,sum=2;  
    Scanner scn=new Scanner(System.in);
    System.out.println("Enter ");
    num=scn.nextInt();
    for(i=1;i<=num;i++){
       if(i==1){
        sum=sum*i;
       }
      System.out.println("sum "+sum);
      sum=sum*2;
    }
    
    }
}
