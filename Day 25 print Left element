import java.util.*;
public class Main {
	public static void main(String[] args) {
		int x=0,temp=0;
		Scanner s=new Scanner(System.in);
		int num=s.nextInt();
		int []arr= new int[num];
		for(int i=0;i<num;i++){
			arr[i]=s.nextInt();
	}
	Arrays.sort(arr);
System.out.println("Left element is"+arr[(((int)num-1)/2)]);
}
}
