import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
       int num,squar,digit=1;
        Scanner sc=new Scanner(System.in);
        num=sc.nextInt();
        while(num>=1)
        {
        digit=num%10;
        squar=digit*digit;
System.out.println("The squar:"+squar);
          num/=10;
        }
            
    }
}
