import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
int num,temp=0,sum=0,digit=0;
Scanner scn=new Scanner(System.in);
num=scn.nextInt();
      temp=num;
     while(num>0){
    digit=num%10; 
    sum=sum*10+digit;
    num/=10;
    
         }
         if(temp==sum)
   System.out.println("number is palindrome");
   else{
       System.out.println("number is not palindrome");
   }
   
    }
}
