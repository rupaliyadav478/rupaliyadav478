import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
      int num,n=0;
      System.out.println("Enter number");
Scanner scn=new Scanner(System.in);
num=scn.nextInt();
     
for(int j=1;j<=num;j++)
  {
      n=j*j;
      System.out.println("The squar :"+n);
  } 
  }
}       


