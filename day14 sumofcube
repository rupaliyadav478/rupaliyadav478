import java.util.*;
class HelloWorld {
 public static void main(String[] args) {
 int digit=0,num,sum=0,temp=0,add=0;
 Scanner scn=new Scanner(System.in);
 System.out.println("Enter the num:");
 num=scn.nextInt();
  while(num>0) {
      digit=num%10;
      sum=sum*10+digit;
      num/=10;
  }
 while(sum>0) {
     temp=sum%10;
     add=add+(temp*temp*temp);
     sum/=10;
 }
System.out.println("The sum of cube:"+add);
 }
    }


