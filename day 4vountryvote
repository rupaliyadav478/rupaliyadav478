package age;
public class Age {
    public static void main(String[] args) {
        int age=23;
        if(age>=18)
        {
          System.out.println("Yes:person is eligible to vote");
        }
        else
        {
         System.out.println("No:persone is not eligible to vote");
        }
    }
    //output:Yes:persone is eligible to vote
}
