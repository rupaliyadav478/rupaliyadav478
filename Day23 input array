import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
      Scanner s = new Scanner(System.in);
      int size = s.nextInt();
      int Array[] = new int [size];
      for(int i=0; i<size; i++) {
      Array[i]=i;
      }
       System.out.println(Arrays.toString(Array));
    }
}
