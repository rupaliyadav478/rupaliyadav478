import java.util.*;
class LCM {
    public static void main(String[] args) {
    int num1 , num2, lcm;
    System.out.println("enter the num1 and num2");
    Scanner sc=new Scanner(System.in);
    num1=sc.nextInt();
       num2=sc.nextInt();
     lcm = (num1 > num2) ? num1 : num2;
while(true) 
{ 
    if( lcm % num1 == 0 && lcm % num2 == 0 )
    {
   System.out.println("The LCM of ."+lcm);
    break;
   } 
++lcm;
} 
}
}
     
