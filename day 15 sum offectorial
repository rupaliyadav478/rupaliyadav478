import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
int num,total=0;
Scanner scn=new Scanner(System.in);
num=scn.nextInt();
  while(num>0){
   int  temp=num;
  int digit=1,sum=1;
     while(temp>0){
    digit=temp%10;   
    sum=sum*digit;
    temp/=10;
         }
         total=total+sum;
          num/=10;
  }
   System.out.println(+total);
    }
}
