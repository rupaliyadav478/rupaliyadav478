import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
   for(int i=1;i<=11;i=i+2){
       for(int j=1;j<=i;j++){
           System.out.print("*");
       }
       System.out.print("\n");
   }
   for(int i=11;i>=1;i=i-2){
       for(int j=1;j<=i;j++){
           System.out.print("*");
       }
        System.out.print("\n");
   }
    }
}
