import java.util.*;
class Countsqure{
    public static void main(String[]args){
        int num,count=0;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number:");
        num=sc.nextInt();
        for(int i=1;i<num;i++){
            if(i*i<num){
            count++;
            }
            
        }
        System.out.println("the count squre is:"+count);
    }
}
