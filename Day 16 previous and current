import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
      int num,n=0,sum=0,j=0;
      System.out.println("Enter number");
Scanner scn=new Scanner(System.in);
num=scn.nextInt();

for(int i=1;i<=num;i++)
  {
      for(;j<i;j++){
        sum=i+j;
      }
      System.out.println("previous+current :"+sum);
  }
  }
}       
