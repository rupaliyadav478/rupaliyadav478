import java.io.*;
import java.util.*;
public class Array {
  public static void main(String[]args) {
System.out.println("enter the value of array");
 Scanner s = new Scanner(System.in);
      int size = s.nextInt();
      int j=0;
      int Array[] = new int [size];
      for(int i=0;i<size;i++){
      Array[i]=s.nextInt();
      j=i;
    }
 System.out.println((Array[0]+","+Array[j]));
}
}
