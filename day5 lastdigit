package digit;
import java.util.*;
public class Digit {
    public static void main(String[] args) {
       int digit ,lastdigit;
       Scanner scn=new Scanner(System.in);
        System.out.println("Enter digit");
        lastdigit=scn.nextInt();
        digit=lastdigit%10;
        System.out.println("The last digit:"+digit);
        
    }
}
