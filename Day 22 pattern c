import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner (System.in);
     System.out.println("enter the value of number ");
     int num=sc.nextInt();
     for(int i=1;i<=num;i++){
         char ch=(char)('A'+num-i);
         char ch1=ch;
        for(int j=1;j<=i;j++){
            System.out.print(ch1);
            ch1++;
        }
        ch++;
        System.out.print("\n");
        }
    }
}
