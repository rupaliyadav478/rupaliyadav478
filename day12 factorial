
package factorial;
import java.util.*;
public class FACTORIAL {

    public static void main(String[] args) {
        int num,fact=1;
        Scanner scn=new Scanner(System.in);
        System.out.println("Enter the number :");
        num=scn.nextInt();
        for(int i=1;i<=num;i++)
        {
            fact=fact*i;
        }
        System.out.println("The factorial="+fact
                );
    }
}
