import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int num=s.nextInt();
		int []arr= new int[num];
		for(int i=0;i<num;i++){
			arr[i]=s.nextInt();
		}
		for(int j=0;j<num;j++){
			if(arr[j]==j+1){
				System.out.println(arr[j]);
	}
}
	}
}
