import java .util.*;
public class repaced {

    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the n number:");
        int n=sc.nextInt(),digit=0,res=0,i=1;   
        while(n>0){
        digit=n%10;
        if(digit==0)
        {
            digit=5;
        }
         res=res+digit*i;
         n=n/10;
         i=i*10;
        }
        System.out.println(res);
        }
    }
